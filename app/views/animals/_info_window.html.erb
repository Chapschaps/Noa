
 <div class="card-animal-map">
  <h2><%= animal.name %></h2>
  <p><%= animal.location %></p>
  <% if animal.photo.attached? %>
    <%= image_tag animal.photo, :style => "width: 100%"%>
  <% else %>
    <%= image_path "test.jpg", :style => "width: 100%"%>
  <% end %>
  <%= link_to "", animal_path(animal), class: "card-link" %>
</div>

